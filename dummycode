 $apiKey = urlencode('V9YcUuFH/wg-6TVbDCVtNPZnaNfoxqly54A9BhCpY4');
        // Message details
        $numbers = array(916361375923);
        $sender = urlencode('Jihhrd');
        $msgTemp = "Dear kk, Your OTP for login to the JIH portal is 1234. Valid for 30 minutes. Please do not share this OTP. Regards, JIHHRD.com";
        $message = rawurlencode($msgTemp);

        $numbers = implode(',', $numbers);

        // Prepare data for POST request
        $data = array('apikey' => $apiKey, 'numbers' => $numbers, "sender" => $sender, "message" => $message);
        // Send the POST request with cURL
        $ch = curl_init('https://api.textlocal.in/send/');
        curl_setopt($ch, CURLOPT_POST, true);
        curl_setopt($ch, CURLOPT_POSTFIELDS, $data);
        curl_setopt($ch, CURLOPT_RETURNTRANSFER, true);
        $response = curl_exec($ch);
        dd($response);
        curl_close($ch);